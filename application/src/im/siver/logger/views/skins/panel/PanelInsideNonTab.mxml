<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
             width="422" height="414">
    <s:states>
        <s:State name="normal"/>
        <s:State name="disabled"/>
    </s:states>
    <!--1 px border for all content-->
    <s:Rect width="100%" height="100%" y="24">
        <s:fill>
            <s:SolidColor color="#b2b2b2"/>
        </s:fill>
    </s:Rect>
    <!--tab header background-->
    <s:Rect width="100%" height="24" y="0" left="0" right="0">
        <s:fill>
            <s:SolidColor color="#363636"/>
        </s:fill>
    </s:Rect>
    <!--content background-->
    <s:Rect width="100%" height="100%" left="1" right="1" top="24" bottom="1">
        <s:fill>
            <s:SolidColor color="#f5f5f5"/>
        </s:fill>
    </s:Rect>
    <s:Group id="contentGroup" left="1" right="1" top="24" bottom="1">
    </s:Group>
</s:SparkSkin>
